{
  "type": "think-admin-plugin",
  "name": "zoujingli/think-plugs-payment",
  "homepage": "https://thinkadmin.top",
  "description": "Payment Plugin for ThinkAdmin",
  "authors": [
    {
      "name": "Anyon",
      "email": "zoujingli@qq.com"
    }
  ],
  "version": "dev-master",
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "zoujingli/think-plugs-admin": "^1.0|dev-master",
    "zoujingli/think-plugs-account": "^1.0|dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "plugin\\payment\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "think\\admin\\tests\\": "tests/"
    }
  },
  "extra": {
    "think": {
      "services": [
        "plugin\\payment\\Service"
      ]
    },
    "plugin": {
      "copy": {
        "stc/database": "database/migrations"
      }
    },
    "minimum-stability": "dev",
    "config": {
      "type": "plugin",
      "name": "多端支付管理",
      "license": [
        "VIP"
      ],
      "homepage": "https://thinkadmin.top",
      "document": "https://thinkadmin.top/plugin/think-plugs-payment.html"
    }
  },
  "config": {
    "allow-plugins": {
      "zoujingli/think-install": true
    }
  }
}
